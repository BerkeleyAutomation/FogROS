
sudo apt install -y docker.io
sudo docker pull keplerc/grasp_image:gpu
sudo docker run -d --network host --rm keplerc/grasp_image:gpu rosrun gqcnn-ros gqcnn_ros_client.py --camera_intr=/root/catkin_ws/src/gqcnn-ros/data/calib/phoxi.intr --depth_image=/root/catkin_ws/src/gqcnn-ros/data/examples/phoxi_clutter/depth_0.npy
sudo docker run --network host --gpus all --rm keplerc/grasp_image:gpu
